{
  "name": "sound-scape-explorer",
  "scripts": {
    "audio": "pnpm serve --cors",
    "front": "cd front && pnpm dev",
    "front:audio": "concurrently -P --kill-others 'pnpm front' 'pnpm audio {1}'",
    "dataframe:csv": "python3 processing/processing/actions/run_dataframe.py",
    "dev": "pnpm front",
    "dev:audio": "pnpm front:audio",
    "install:all": "pnpm install:audio && pnpm install:front && pnpm install:processing",
    "install:audio": "sudo npm -g i serve",
    "install:front": "cd front && pnpm i && cd ..",
    "install:processing": "cd processing && pip3 install -r requirements.txt && cd ..",
    "migrate:v8": "python3 processing/processing/actions/run_migration_v8.py",
    "process": "pnpm process:all",
    "process:all": "python3 processing/processing/actions/_all.py",
    "process:all-actions": "python3 processing/processing/actions/_all_actions.py",
    "process:all-but-files": "python3 processing/processing/actions/_all_but_files.py",
    "process:all-to-groups": "python3 processing/processing/actions/_all_to_groups.py",
    "process:config": "python3 processing/processing/actions/run_config.py",
    "process:files": "python3 processing/processing/actions/run_files.py",
    "process:groups": "python3 processing/processing/actions/run_groups.py",
    "process:autocluster": "python3 processing/processing/actions/run_autocluster.py",
    "process:reducers": "python3 processing/processing/actions/run_reducers.py",
    "process:indicators": "python3 processing/processing/actions/run_indicators.py",
    "process:volumes": "python3 processing/processing/actions/run_volumes.py",
    "process:matrices": "python3 processing/processing/actions/run_matrices.py",
    "process:pairings": "python3 processing/processing/actions/run_pairings.py",
    "release:prepare": "pnpm release:prepare:archive-examples",
    "release:prepare:archive-examples": "bin/archive-examples.sh",
    "set:path:windows": "bin/set-python-path.ps1",
    "set:path:macos": "pnpm set:path:unix",
    "set:path:debian": "pnpm set:path:unix",
    "set:path:ubuntu": "pnpm set:path:unix",
    "set:path:unix": "bin/set-python-path.sh",
    "start": "pnpm dev",
    "test:git:unix": "bin/test-git.sh",
    "test:git:windows": "bin/test-git.ps1",
    "test:install:unix": "pnpm test:git:unix && pnpm test:python:unix && pnpm test:nodejs:unix && pnpm test:pnpm:unix",
    "test:install:windows": "pnpm test:git:windows && pnpm test:python:windows && pnpm test:nodejs:windows && pnpm test:pnpm:windows",
    "test:nodejs:unix": "bin/test-nodejs.sh",
    "test:nodejs:windows": "bin/test-nodejs.ps1",
    "test:pnpm:unix": "bin/test-pnpm.sh",
    "test:pnpm:windows": "bin/test-pnpm.ps1",
    "test:python:unix": "bin/test-python.sh",
    "test:python:windows": "bin/test-python.ps1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "rimraf": "^5.0.1",
    "serve": "^14.2.0"
  },
  "version": "9.0.0"
}
