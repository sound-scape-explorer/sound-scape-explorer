{
  "name": "sound-scape-explorer",
  "scripts": {
    "install": "./install.sh",
    "dev": "concurrently './run-back.sh' './run-front.sh'",
    "process": "yarn process:all",
    "process:all": "python3 processing/processing/actions/all.py",
    "process:config": "python3 processing/processing/actions/config.py",
    "process:extract": "python3 processing/processing/actions/extract_features.py",
    "process:group": "python3 processing/processing/actions/group_features.py",
    "process:reduce": "python3 processing/processing/actions/reduce_groups.py",
    "release:prepare": "yarn release:prepare:archive-examples",
    "release:prepare:archive-examples": "bin/archive-examples.sh",
    "start": "yarn dev"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "rimraf": "^3.0.2"
  },
  "version": "5.17.0"
}
