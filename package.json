{
  "name": "sound-scape-explorer",
  "scripts": {
    "install": "./install.sh",
    "clean:all": "rimraf -r sample/features sample/generated",
    "clean:config": "rimraf -r sample/generated/ghost-config.json",
    "clean:coverings": "rimraf -r sample/generated/pairwise",
    "clean:extract": "rimraf -r sample/features",
    "clean:features": "rimraf -r sample/generated/features",
    "clean:indicators": "rimraf -r sample/generated/indicators",
    "clean:indicators:aci": "rimraf -r sample/generated/indicators/ACI.json",
    "clean:indicators:adi": "rimraf -r sample/generated/indicators/ADI.json",
    "clean:indicators:bi": "rimraf -r sample/generated/indicators/BI.json",
    "clean:indicators:hf": "rimraf -r sample/generated/indicators/Hf.json",
    "clean:indicators:ndsi": "rimraf -r sample/generated/indicators/NDSI.json",
    "clean:indicators:ht": "rimraf -r sample/generated/indicators/Ht.json",
    "clean:indicators:leq-t": "rimraf -r sample/generated/indicators/Leq_T.json",
    "clean:indicators:med": "rimraf -r sample/generated/indicators/MED.json",
    "clean:umaps": "rimraf -r sample/generated/umap",
    "clean:volumes": "rimraf -r sample/generated/single",
    "compute:all": "processing/run.sh sample compute",
    "compute:all-but-coverings": "processing/run.sh sample all-but-covering",
    "compute:all-but-volumes": "processing/run.sh sample all-but-volume",
    "compute:config": "processing/run.sh sample config",
    "compute:config:test": "processing/run.sh sample config-test",
    "compute:coverings": "processing/run.sh sample compute-covering",
    "compute:features": "processing/run.sh sample compute-features",
    "compute:indicators": "processing/run.sh sample compute-indicators",
    "compute:indicators:aci": "processing/run.sh sample compute-indicators-aci",
    "compute:indicators:adi": "processing/run.sh sample compute-indicators-adi",
    "compute:indicators:bi": "processing/run.sh sample compute-indicators-bi",
    "compute:indicators:hf": "processing/run.sh sample compute-indicators-hf",
    "compute:indicators:ndsi": "processing/run.sh sample compute-indicators-ndsi",
    "compute:indicators:ht": "processing/run.sh sample compute-indicators-ht",
    "compute:indicators:leq-t": "processing/run.sh sample compute-indicators-leq-t",
    "compute:indicators:med": "processing/run.sh sample compute-indicators-med",
    "compute:test": "processing/run.sh sample test",
    "compute:umaps": "processing/run.sh sample compute-umap",
    "compute:volumes": "processing/run.sh sample compute-volume",
    "dev": "concurrently './run-back.sh' './run-front.sh'",
    "extract": "processing/run.sh sample extract",
    "populate:config": "processing/run.sh sample config-populate-files",
    "config:write:indicators": "processing/run.sh sample config-write-indicators",
    "process": "yarn process:all",
    "process:all": "processing/run.sh sample all",
    "process:clean": "processing/clean.sh sample",
    "release:prepare": "yarn release:prepare:archive-examples",
    "release:prepare:archive-examples": "bin/archive-examples.sh",
    "start": "yarn dev"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "rimraf": "^3.0.2"
  },
  "version": "5.7.0"
}
