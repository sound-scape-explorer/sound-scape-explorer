{
  "name": "sound-scape-explorer",
  "scripts": {
    "install": "./install.sh && yarn process",
    "clean:config": "rimraf -r sample/generated/ghost-config.json",
    "clean:coverings": "rimraf -r sample/generated/pairwise",
    "clean:extract": "rimraf -r sample/features",
    "clean:features": "rimraf -r sample/generated/features",
    "clean:umaps": "rimraf -r sample/generated/umap",
    "clean:volumes": "rimraf -r sample/generated/single",
    "compute:all": "yarn process:compute",
    "compute:config": "yarn process:config",
    "compute:coverings": "yarn process:compute:covering",
    "compute:features": "processing/run.sh sample compute-features",
    "compute:umaps": "yarn process:compute:umap",
    "compute:volumes": "yarn process:compute:volumes",
    "dev": "concurrently './run-back.sh' './run-front.sh'",
    "config:columns": "processing/run.sh sample config-populate-columns",
    "config:files": "processing/run.sh sample config-populate-files",
    "config:meta": "yarn config:columns",
    "process": "yarn process:all",
    "process:all": "processing/run.sh sample all",
    "process:all-but-covering": "processing/run.sh sample all-but-covering",
    "process:all-but-volume": "processing/run.sh sample all-but-volume",
    "process:extract": "processing/run.sh sample extract",
    "process:compute": "processing/run.sh sample compute",
    "process:compute:volume": "yarn process:compute:volumes",
    "process:compute:volumes": "processing/run.sh sample compute-volume",
    "process:compute:covering": "processing/run.sh sample compute-covering",
    "process:compute:umap": "processing/run.sh sample compute-umap",
    "process:config": "processing/run.sh sample config",
    "process:config:test": "processing/run.sh sample config-test",
    "process:clean": "processing/clean.sh sample",
    "process:populate-config": "yarn config:columns",
    "process:show:features": "processing/run.sh sample show-features",
    "start": "yarn dev"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "rimraf": "^3.0.2"
  },
  "version": "3.15.2"
}
