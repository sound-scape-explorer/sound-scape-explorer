{
  "name": "sound-scape-explorer",
  "scripts": {
    "audio": "pnpm serve --cors -p 5531",
    "front": "cd front && pnpm dev",
    "front:audio": "concurrently -P --kill-others 'pnpm front' 'pnpm audio {1}'",
    "dev": "pnpm front",
    "dev:audio": "pnpm front:audio",
    "install:all": "pnpm install:audio && pnpm install:front && pnpm install:processing",
    "install:audio": "sudo npm -g i serve",
    "install:front": "cd front && pnpm i && cd ..",
    "install:processing": "cd processing && pip3 install -r requirements.txt && cd ..",
    "_process": "python3 processing/main.py",
    "process": "pnpm process:all",
    "process:all": "pnpm _process --action all",
    "process:all-actions": "pnpm _process --action all-actions",
    "process:all-but-files": "pnpm _process --action all-but-files",
    "process:all-to-groups": "pnpm _process --action all-to-groups",
    "process:autocluster": "pnpm _process --action autocluster",
    "process:config": "pnpm _process --action config",
    "process:dataframe": "pnpm _process --action dataframe",
    "process:files": "pnpm _process --action files",
    "process:groups": "pnpm _process --action groups",
    "process:indicators": "pnpm _process --action indicators",
    "process:matrices": "pnpm _process --action matrices",
    "process:migrate:v8": "pnpm _process --action migrate-v8",
    "process:pairings": "pnpm _process --action pairings",
    "process:reducers": "pnpm _process --action reducers",
    "process:volumes": "pnpm _process --action volumes",
    "release:prepare": "pnpm release:prepare:archive-examples",
    "release:prepare:archive-examples": "bin/archive-examples.sh",
    "start": "pnpm dev",
    "test:git:unix": "bin/test-git.sh",
    "test:git:windows": "bin/test-git.ps1",
    "test:install:unix": "pnpm test:git:unix && pnpm test:python:unix && pnpm test:nodejs:unix && pnpm test:pnpm:unix",
    "test:install:windows": "pnpm test:git:windows && pnpm test:python:windows && pnpm test:nodejs:windows && pnpm test:pnpm:windows",
    "test:nodejs:unix": "bin/test-nodejs.sh",
    "test:nodejs:windows": "bin/test-nodejs.ps1",
    "test:pnpm:unix": "bin/test-pnpm.sh",
    "test:pnpm:windows": "bin/test-pnpm.ps1",
    "test:python:unix": "bin/test-python.sh",
    "test:python:windows": "bin/test-python.ps1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "rimraf": "^5.0.1",
    "serve": "^14.2.0"
  },
  "version": "9.0.0"
}
