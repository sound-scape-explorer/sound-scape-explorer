{
  "name": "sound-scape-explorer",
  "scripts": {
    "dev": "cd front && pnpm dev",
    "audio": "pnpm serve --cors",
    "extract:v9:files_features": "python3 processing/processing/actions/extract_v9_files_features.py",
    "migrate:v8": "python3 processing/processing/actions/run_migration_v8.py",
    "process": "pnpm process:all",
    "process:all": "python3 processing/processing/actions/_all.py",
    "process:all-actions": "python3 processing/processing/actions/_all_actions.py",
    "process:all-but-files": "python3 processing/processing/actions/_all_but_files.py",
    "process:all-to-groups": "python3 processing/processing/actions/_all_to_groups.py",
    "process:config": "python3 processing/processing/actions/run_config.py",
    "process:dataframe": "python3 processing/processing/actions/run_dataframe.py",
    "process:files": "python3 processing/processing/actions/run_files.py",
    "process:groups": "python3 processing/processing/actions/run_groups.py",
    "process:autocluster": "python3 processing/processing/actions/run_autocluster.py",
    "process:reducers": "python3 processing/processing/actions/run_reducers.py",
    "process:indicators": "python3 processing/processing/actions/run_indicators.py",
    "process:volumes": "python3 processing/processing/actions/run_volumes.py",
    "process:matrices": "python3 processing/processing/actions/run_matrices.py",
    "process:pairings": "python3 processing/processing/actions/run_pairings.py",
    "release:prepare": "pnpm release:prepare:archive-examples",
    "release:prepare:archive-examples": "bin/archive-examples.sh",
    "start": "pnpm dev"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "rimraf": "^5.0.0",
    "serve": "^14.2.0"
  },
  "version": "9.1.0"
}
