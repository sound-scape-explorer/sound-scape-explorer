    "highcharts-vue": "^1.4.0",
    "html2canvas": "^1.4.1",
    "simple-statistics": "^7.8.3",
    "three": "^0.150.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "wav-encoder": "^1.3.0",
    "wavesurfer.js": "^6.6.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@types/chroma-js": "^2.4.0",
    "@types/colormap": "^2.3.1",
    "@types/three": "^0.149.0",
