    "naive-ui": "^2.34.3",
    "rimraf": "^4.4.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.58.3",
    "sass-reset": "^1.0.4",
    "typescript": "^4.9.5",
    "vfonts": "^0.0.3",
    "vite": "^4.1.4",
    "vite-plugin-top-level-await": "^1.3.0",
